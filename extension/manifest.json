{
    "manifest_version": 3,
    "name": "topUp",
    "version": "0.0.0",
    "description": "A top-up application with validation and dark mode.",
    "action": {
        "default_popup": "topUp.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "64": "icons/icon64.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "storage"
    ]
}